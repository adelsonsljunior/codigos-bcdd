USE ATV2;

SELECT * FROM CLIENTE
ORDER BY NOME;

SELECT * FROM CLIENTE
ORDER BY NUMERO  DESC;

SELECT * FROM CLIENTE
WHERE NUMERO IS NOT NULL;

SELECT * FROM PRODUTO
WHERE VALOR BETWEEN 1 AND 10;

SELECT * FROM PRODUTO
ORDER BY VALOR;

SELECT * FROM MUNICIPIO
WHERE NOME LIKE 'C%';

SELECT * FROM MUNICIPIO
WHERE NOME LIKE '%E';

SELECT * FROM PRODUTO
WHERE DESCRICAO  LIKE 'N%';

SELECT * FROM PRODUTO
WHERE QUANT BETWEEN 20 AND 50;

SELECT * FROM PRODUTO
WHERE QUANT_MINIMA IN (10, 20);















